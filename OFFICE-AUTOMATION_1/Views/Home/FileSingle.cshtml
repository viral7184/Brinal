    <div class="alert alert-success alert-dismissible" style="display: none">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
        <h4><i class="icon fa fa-check"></i> Alert!</h4> File is added successfully
    </div>
    <!-- Modal -->
    <div class="row">
        <div class="col-lg-12">
            <div class="box box-warning">
                <div class="box-header with-border">
                    <a href="~/editfile/@Model.ID" @*onclick="OpenDialog(@Model.ID)"*@ type="button" class="btn btn-primary" style="float: right;">Edit File</a>
                </div>
                <!-- /.box-header -->
                <div class="box-body box-detail">
                    <p class="file-title"><strong>File Details</strong></p>
                    <div class="row">
                        <div class="col-xs-12 col-sm-6" style="padding-left: 40px">
                            <dl class="dl-horizontal">

                                <dt>Customer Name</dt>
                                <dl>
                                    @Model.CUSTOMERNAME
                                </dl>
                                <dt>No Of Year</dt>
                                <dl id="NO_OF_YEAR_@Model.ID">
                                    @if (Model.NO_OF_YEAR != null)
                                    {
                                        @Model.NO_OF_YEAR
                                    }
                                    else
                                    {
                                        <span>-</span>
                                    }
                                </dl>
                                <dt>Inward Date</dt>
                                <dl id="INWARD_DATE_@Model.ID">
                                    @if (Model.INWARD_DATE != null)
                                    {
                                        @Model.INWARD_DATE.Value.ToString("dd/MM/yyyy")
                                    }
                                    else
                                    {
                                        <span>-</span>
                                    }

                                </dl>
                                <dt>Search Fees</dt>
                                <dl id="SEARCH_FEES_@Model.ID">
                                    @if (Model.SEARCH_FEES != null)
                                    {
                                        @Model.SEARCH_FEES
                                    }
                                    else
                                    {
                                        <span>-</span>
                                    }
                                </dl>
                                <dt>Legal Fees</dt>
                                <dl id="LEGAL_FEES_@Model.ID">
                                    @if (Model.LEGAL_FEES != null)
                                    {
                                        @Model.LEGAL_FEES
                                    }
                                    else
                                    {
                                        <span>-</span>
                                    }
                                </dl>
                                <dt>NEC</dt>
                                <dl>
                                    @if (Model.NEC != null)
                                    {
                                        @Model.NEC
                                    }
                                    else
