                                @*<button type="button" class="btn btn-info pull-right" >Edit File Status</button>*@
                                <input type="hidden" name="ID" value="@Model.ID" />
                                <input type="hidden" name="R_NO" value="@Model.R_NO" />
                                <input type="submit" class="btn btn-info pull-right" value="Save Status" />
                                <input type="button" class="btn  pull-right" style="margin-right:3px" data-dismiss="modal" value="cancel" />
                            </div>
                        </form>
                        <!-- /.box-body -->
                    </div>
                </div>
            </div>
        </div>
        <!-- Edit D File status -->
        <div class="modal fade" id="myModalChangeD" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
