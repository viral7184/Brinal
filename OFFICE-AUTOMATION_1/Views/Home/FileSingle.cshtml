@*@model IEnumerable<OFFICE_AUTOMATION_1.Models.file>*@
@model OFFICE_AUTOMATION_1.Models.file
@{
    ViewBag.Title = "FileSingle";

}


<!-- Content Header (Page header) -->
<!-- Main content -->
<section class="content">
    <!-- Info boxes -->
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">File : @Model.R_NO , Customer : @Model.CUSTOMERNAME , Agent : @Model.agent_master.NAME</h3>
        </div>
        <!-- Edit P File status -->
        <div class="modal fade" id="myModalChangeP" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                        <h4 class="modal-title" id="myModalLabel">Is title created ?</h4>
                    </div>
                    <div class="modal-body">
                        <!-- /.box-header -->
                        <form role="form" id="mfrm1" method="post" enctype="multipart/form-data" action="../ChangeStatusNow">
                            <!-- text input -->                            
                                <div class="form-group ">
                                    <label>Title Upload</label>
                                    <input type="file" name="FILE_NAME" id="filess">
                                     @*<span class="help-block error">* User Name Already Exist</span>*@ 
                                </div>                           
                            <input type="hidden" name="ID" value="@Model.ID" />
                            <input type="hidden" name="R_NO" value="@Model.R_NO" />
                            <div class="form-group">
                                <div class="radio">
                                    <label>
                                        <input type="radio" name="T" id="PG" value="gray" @((ViewBag.stsclr.T == "gray") ? "checked" : "")> Gray
                                    </label>
                                    <label style="margin-left: 10px">
                                        <input type="radio" name="T" id="PGN" value="green" @((ViewBag.stsclr.T == "green") ? "checked" : "")> Green
                                    </label>
                                </div>
                            </div>
                            <div class="box-footer">
                                <input type="submit" class="btn btn-info pull-right" value="Save Status" />
                                <input type="button" class="btn pull-right" style="margin-right:3px" data-dismiss="modal" value="cancel" />
                            </div>
                        </form>
                        <!-- /.box-body -->
                    </div>
                </div>
            </div>
        </div>
        <!-- Edit S File status -->
        <div class="modal fade" id="myModalChangeS" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                        <h4 class="modal-title" id="myModalLabel">Edit Search Status (S)</h4>
                    </div>
                    <div class="modal-body">
                        <!-- /.box-header -->
                        <form role="form" id="mfrm" method="post" action="../ChangeStatusNow">
                            <!-- text input -->
                            <div class="form-group" id="abc">
                                <label>Change Status S</label>
                                <div class="radio">
                                    <label>
                                        <input type="radio" onclick="Receicer1()" name="S" @((ViewBag.stsclr.S == "gray") ? "checked" : "") id="SG" value="gray" checked> Gray
                                    </label>
                                    <label style="margin-left: 10px">
                                        <input type="radio" onclick="Receicer1()" name="S" @((ViewBag.stsclr.S == "blue") ? "checked" : "") id="SB" value="blue"> Blue
                                    </label>
                                    <label style="margin-left: 10px">
                                        <input type="radio" onclick="Receicer()" name="S" @((ViewBag.stsclr.S == "green") ? "checked" : "") id="SGN" value="green"> Green
                                    </label>
                                </div>
                            </div>
                            <div class="form-group hidden" id="receiver">
                                <label>Recieved To</label>
                                <select name="RECEIVER" id="RECEIVER" class="form-control select2" style="width: 30%;">
                                    <option selected value="1">NO RECEIVER</option>
                                    @foreach (var item in ViewBag.emp)
                                    {
                                      <option value="@item.ID">@item.NAME</option>
                                    }
                                </select>
                            </div>
                            <div class="box-footer">
                                <input type="hidden" name="ID" value="@Model.ID" />
                                <input type="hidden" name="R_NO" value="@Model.R_NO" />
                                <input type="submit" id="submit" class="btn btn-info pull-right" value="Save Status" />
                                <input type="button" class="btn pull-right" style="margin-right:3px" data-dismiss="modal" value="Cancle" />
                                @*<button type="submit" class="btn btn-info pull-right">Edit File Status</button>*@
                            </div>
                        </form>
                        <!-- /.box-body -->
                    </div>
                </div>
            </div>
        </div>
        <!-- Edit Q File status -->
        <div class="modal fade" id="myModalChangeQ" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                        <h4 class="modal-title" id="myModalLabel">Edit File Status (Q)</h4>
                    </div>
                    <div class="modal-body">
                        <!-- /.box-header -->
                        <form role="form" id="mfrm3" method="post" action="../ChangeStatusNow">
                            <!-- text input -->
                            <!-- text input -->
                            <div class="form-group ">
                                <label>Query</label>
                                @*<input type="text" name="QUERY_REMARK" value="@item.QUERY_REMARK" class="form-control" placeholder="Add Query Here">*@
                                <select name="QUERY_REMARK" id="QUERY_REMARK" class="form-control select2" style="width: 100%">
                                    <option value="Query Not Generate">Select Query</option>
                                    <optgroup label="Sale Deed">
                                        <option value="Party's Sale Deed">Party's Sale Deed</option>
                                        <option value="Previous Party's Sale Deed">Previous Party's Sale Deed</option>
                                        <option value="Original Sale Deed of party when necessary">Original Sale Deed of party when necessary</option>
                                    </optgroup>
                                    <optgroup label="LOD.">
                                        <option value="Original LOD with new or latest date">Original LOD with new or latest date</option>
                                        <option value="Xerox LOD with new or latest date">Xerox LOD with new or latest date</option>
                                    </optgroup>
                                    <optgroup label="Master file">
                                        <option value="Master file">Master file</option>
                                    </optgroup>
                                    <optgroup label="N.A. Order">
                                        <option value="N.A. Order">N.A. Order </option>
                                    </optgroup>
                                    <optgroup label="Construction Permission">
                                        <option value="Construction Permission">Construction Permission</option>
                                    </optgroup>
                                    <optgroup label="Agreement to sale">
                                        <option value="Original agreement to sale">Original agreement to sale</option>
                                        <option value="Copy of a agreement to sale">Copy of a agreement to sale</option>
                                    </optgroup>
                                </select>
                                <!-- <span class="help-block error">* User Name Already Exist</span> -->
                            </div>
                            <div class="form-group ">
                                <label>sms msg content</label>
                                <input type="text" name="SMS_CONTENT" id="smscontent" class="form-control" placeholder="Add sms_msg">
                                <!-- <span class="help-block error">* User Name Already Exist</span> -->
                            </div>
                            <div class="form-group ">
                                <label>mail msg content </label>
                                <input type="text" name="EMAIL_CONTENT" id="Emailcontent" class="form-control" placeholder="Add message">
                                <!-- <span class="help-block error">* User Name Already Exist</span> -->
                            </div>
                            <!-- text input -->
                            <div class="form-group">
                                <input type="checkbox" name="OV" id="ov" value="Yes" />
                                <label for="exampleInputFile">OV</label>
                                <input type="checkbox" name="LOD" id="lod" value="Yes" />
                                <label for="exampleInputFile">LOD</label>
                            </div>

                            <!-- <span class="help-block error">* User Name Already Exist</span> -->
                            <!-- text input -->

                            <div class="form-group">
                                <label>Change Status Q</label>
                                <div class="radio">
                                    <label>
                                        <input type="radio" name="Q" @((ViewBag.stsclr.Q == "gray") ? "checked" : "") id="QG" value="gray" checked> Gray
                                    </label>
                                    <label style="margin-left: 10px">
                                        <input type="radio" name="Q" @((ViewBag.stsclr.Q == "blue") ? "checked" : "") id="QB" value="blue"> Blue
                                    </label>
                                    <label style="margin-left: 10px">
                                        <input type="radio" name="Q" @((ViewBag.stsclr.Q == "green") ? "checked" : "") id="QGN" value="green"> Green
                                    </label>
                                </div>
                            </div>
                            <div class="box-footer">
                                @*<button type="button"  class="btn btn-info pull-right">Edit File Status</button>*@
                                <input type="hidden" name="SENT_TO" value="@Model.A_ID" />
                                <input type="hidden" name="ID" value="@Model.ID" />
                                <input type="hidden" name="R_NO" value="@Model.R_NO" />
                                <input type="hidden" name="A_ID" value="@Model.A_ID" />
                                <button type="submit" class="btn btn-info pull-right">Save Status</button>
                                <input type="button" class="btn  pull-right" style="margin-right:3px" data-dismiss="modal" value="Cancle" />
                            </div>
                        </form>
                        <!-- /.box-body -->
                    </div>
                </div>
            </div>
        </div>

